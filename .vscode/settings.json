{
    "editor.codeActionsOnSave":{
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "never"
    },
    "search.exclude": {
        "**/node_modules": true,
        "package-lock.json": true
    },
    "editor.tabSize": 2,
    "eslint.validate": [
        "javascript"
    ]
}